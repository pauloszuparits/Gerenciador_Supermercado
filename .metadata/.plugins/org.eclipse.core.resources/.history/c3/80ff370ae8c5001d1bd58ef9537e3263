package main;

public class Erro {
	private String descricaoErro;
	public Erro(String descricaoErro) {
		super();
		this.descricaoErro = descricaoErro;		
	}
	public String getDescricaoErro() {
		return descricaoErro;
	}
	public void setDescricaoErro(String descricaoErro) {
		this.descricaoErro = descricaoErro;
	}
	
	
	
}
