package main;
import java.sql.*;
public class main {
	
	public static void main(String[] args) throws SQLException {
		
		Funcoes funcoes = new Funcoes();
		Conexao conexaoComMysql = new Conexao();
		Produto testeJava = new Produto("TesteJava9090", 99.99, 112.60);
		
		Statement declaracaoConexao = conexaoComMysql.getConection().createStatement();	
		
		funcoes.InsereProduto(testeJava, declaracaoConexao);
		
		
	}

}
